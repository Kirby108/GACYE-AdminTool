[
    {
      "name": "default",
      "type": "mysql",
      "database": "default",
      "port": 3306,
      "autoSchemaSync": true,
      "entities": [
        "src/models/**/*{.js,.ts}"
      ],
      "migrations": ["src/migrations/**/*{.js,.ts}"],
      "cli": {
        "migrationsDir": "src/migrations/"
      }
    }
  ]